{
  "name": "klever_global_corp_website",
  "version": "1.0.0",
  "description": "Klever.global corporate website",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src",
    "version": "webpack -v",
    "dev": "rm -rf dist/ && run-p dev:**",
    "dev:hugo": "hugo --baseURL=/ -d ../dist -s site -vw",
    "dev:webpack": "webpack serve --color --config webpack.dev.js",
    "preview": "run-p preview:**",
    "preview:hugo": "npm run dev:hugo -- -D -F",
    "preview:webpack": "npm run dev:webpack",
    "prebuild": "rimraf dist",
    "prod": "npm run build:webpack && npm run build:hugo",
    "prod:preview": "npm run build:webpack && npm run build:hugo:preview",
    "build:hugo": "cross-env NODE_ENV=production hugo -d ../dist -s site -v",
    "build:hugo:preview": "npm run build:hugo -- -D -F",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.prod.js"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-syntax-object-rest-spread": "^7.8.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/register": "^7.12.1",
    "@tailwindcss/aspect-ratio": "^0.2.0",
    "@tailwindcss/forms": "^0.2.1",
    "@tailwindcss/typography": "^0.3.1",
    "alpinejs": "^2.7.3",
    "assets-webpack-plugin": "^3.9.7",
    "autoprefixer": "^10.0.2",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^7.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^3.5.3",
    "css-minimizer-webpack-plugin": "^1.2.0",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.22.1",
    "exports-loader": "^0.7.0",
    "fancy-log": "^1.3.2",
    "file-loader": "^6.2.0",
    "hugo-bin": "^0.63.0",
    "image-minimizer-webpack-plugin": "^2.2.0",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-pngquant": "^9.0.1",
    "imagemin-svgo": "^8.0.0",
    "imports-loader": "^0.8.0",
    "mini-css-extract-plugin": "^0.12.0",
    "node-sass": "^4.14.0",
    "npm-run-all": "^4.1.5",
    "plugin-error": "^1.0.1",
    "postcss": "^8.1.7",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^4.0.4",
    "postcss-preset-env": "^6.7.0",
    "rimraf": "^3.0.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.3.0",
    "tailwindcss": "^2.0.1",
    "terser-webpack-plugin": "^4.2.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.15.0",
    "webpack-cli": "^4.4.0",
    "webpack-dev-middleware": "^4.1.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3",
    "whatwg-fetch": "^3.6.2"
  },
  "repository": {
